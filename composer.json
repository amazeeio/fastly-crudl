{
  "name": "timclifford/fastly_crudl",
  "description": "A PHP wrapper for using Fastly API",
  "keywords": [
    "fastly",
    "api",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Clifford",
      "email": "tim.clifford@amazee.com",
      "homepage": "http://amazeelabs.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "~7.3",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit" : ">=8.0",
    "squizlabs/php_codesniffer": "^3.0",
    "vlucas/phpdotenv": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Fastly\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Fastly\\Tests\\": "tests"
    },
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "test": "phpunit"
  }
}
